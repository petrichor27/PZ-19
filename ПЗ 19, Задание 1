#include <iostream>
#include <fstream>
using namespace std;

bool Sochet(int* res, int n, int k)
{
	for (int i = k - 1; i >= 0; i--)
		if (res[i] < n - k + i)
		{
			res[i]++;
			for (int j = i + 1; j < k; j++)
				res[j] = res[j - 1] + 1;
			return true;
		}
	return false;
}
int fact(int c)
{
	int k = 1;
	for (int i = 1; i < c; ++i)
		k *= k * i;
	return k;
}
int main()
{
	setlocale(LC_ALL, "RUS");
	cout << "Введите p:";
	cin >> p;
	cout << "Введите q:";
	cin >> q;
	int* res = new int[p];
	for (int i = 0; i < p; i++)
		res[i] = i;
	int k = fact(p) / (fact(p - 2) * 2);
	int** reb = new int *[k];
	for (int i = 0; i < k; i++)
		reb[i] = new int[2];
	if (p >= 2)
	{
		int num = 0;
		do
		{
			for (int i = 0; i < 2; i++)
				reb[num][i] = res[i];
			num++;
		} while (Sochet(res, p, 2));

	}

	int* res1 = new int[k];
	for (int i = 0; i < k; i++)
		res1[i] = i;
	bool** ms = new bool* [p];
	for (int i = 0; i < p; i++)
		ms[i] = new bool[p];
	
	if (k >= q)
	{
		int num = 0;
		do
		{
			for (int i = 0; i < p; i++)
		for (int j = 0; j < p; j++)
			ms[i][j] = 0;
			cout << num+1 << ")\n";
			for (int i = 0; i < q; i++)
			{
				ms[reb[res1[i]][0]][reb[res1[i]][1]] = 1;
				ms[reb[res1[i]][1]][reb[res1[i]][0]] = 1;
			}
			for (int i = 0; i < p; i++)
			{
				for (int j = 0; j < p; j++)
					cout << ms[i][j] << " ";
				cout << endl;
			}
			num++;
			cout << endl;
		} while (Sochet(res1, k, q));

	}
	return 0;
}
